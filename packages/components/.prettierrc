{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "proseWrap": "never",
  "overrides": [
    { "files": ".prettierrc", "options": { "parser": "json" } },
    { "files": "*.md", "options": { "proseWrap": "preserve" } }
  ],
  "pluginSearchDirs": false,
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson"]
}
